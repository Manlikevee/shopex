{
  "expo": {
    "name": "shopex",
    "slug": "shopex",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/grp.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/gonp.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/grp.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.veetech.shopex"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8da5df60-a61e-4468-9195-c5903b8c0364"
      }
    }
  }
}
